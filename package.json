{
  "name": "graphology-layout-noverlap",
  "version": "0.0.1",
  "description": "Noverlap anti-collision layout algorithm for graphology.",
  "main": "index.js",
  "files": [
    "*.d.ts",
    "defaults.js",
    "helpers.js",
    "index.js",
    "iterate.js",
    "worker.js",
    "webworker.js"
  ],
  "scripts": {
    "bench": "node bench.js",
    "clean": "rimraf webworker.js",
    "lint": "eslint '*.js'",
    "prepublish": "npm run clean && npm test && npm run lint && npm run template",
    "template": "node ./scripts/template-webworker.js > webworker.js",
    "test": "mocha test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphology/graphology-layout-noverlap.git"
  },
  "keywords": [
    "graph",
    "graphology",
    "layout",
    "force atlas"
  ],
  "author": {
    "name": "Guillaume Plique",
    "url": "http://github.com/Yomguithereal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphology/graphology-layout-noverlap/issues"
  },
  "homepage": "https://github.com/graphology/graphology-layout-noverlap#readme",
  "devDependencies": {
    "@yomguithereal/eslint-config": "^4.0.0",
    "eslint": "^7.3.0",
    "graphology": "0.17.1",
    "graphology-generators": "^0.10.1",
    "graphology-layout": "^0.3.1",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "seedrandom": "^3.0.5"
  },
  "eslintConfig": {
    "extends": "@yomguithereal/eslint-config",
    "globals": {
      "Float32Array": true,
      "Set": true
    }
  },
  "dependencies": {
    "graphology-types": "^0.17.0",
    "graphology-utils": "^1.7.0"
  }
}
